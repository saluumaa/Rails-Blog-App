<h1> Add New Post</h1>
<%= form_with model: @post, url: user_posts_path(@user) do |f| %>
    <div class="field">
        <%= f.label :title %>
        <%= f.text_field :title %>
    </div>
    <div class="field">
        <%= f.label :text %>
        <%= f.text_area :text %>
    </div>
    <div class="actions">
        <%= f.submit %>
    </div>
<% end %> 